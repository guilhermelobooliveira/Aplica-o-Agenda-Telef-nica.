package contato;

import java.util.Scanner;
import javax.swing.JOptionPane;

/**
 *
 * @author Marino
 */
public class ClasseAgenda {
    String contatos[] = new String[0];
    
    public void adicionarnovocontato(){
        JOptionPane.showInputDialog(null, " qual o seu nome? ");
        contatos[0] = "nome";
    }
        
    public String[] contatos(){
        return contatos;
    }
    
     public void excluircontato(){
        JOptionPane.showMessageDialog(null, " contato excluido com sucesso! ");
    }
}
